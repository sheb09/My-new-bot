from selenium import webdriver
from selenium.webdriver.common.keys import Keys
import time

# Inicializar el navegador web (en este caso, Chrome)
driver = webdriver.Chrome()

# Abrir la página web del bot
driver.get("https://www.ejemplo.com")

# Esperar unos segundos para cargar la página
time.sleep(2)

# Encontrar el campo de entrada de texto (por ejemplo, un campo de búsqueda)
search_box = driver.find_element_by_name("q")

# Simular la entrada de texto en el campo de búsqueda
search_box.send_keys("lo que quieres buscar")

# Simular la pulsación de la tecla Enter para enviar la búsqueda
search_box.send_keys(Keys.RETURN)

# Esperar unos segundos para que se carguen los resultados de la búsqueda
time.sleep(2)

# Encontrar y hacer clic en un enlace de los resultados de la búsqueda
search_result = driver.find_element_by_xpath("//a[@class='search-result-link']")
search_result.click()

# Esperar unos segundos para que se cargue la página siguiente
time.sleep(2)

# Cerrar el navegador
driver.quit()
